{
    "name": "ixocreate/ixocreate",
    "description": "IXOCREATE Content Management Framework by @ixolit",
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "ixocreate/framework": "^0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Admin\\": "src/Admin/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
